{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithProcessId",
      "WithThreadId"
    ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{MachineName} {EnvironmentUserName} {EnvironmentName} {Properties} {ThreadId} {ThreadName} {Timestamp:HH:mm:ss} {Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  },
  "INS": {
    "ProvidersPageURL": "https://www.ins-cr.com/centro-de-gestion-de-seguros-personales/pre-autorizacion/red-de-proveedores-nacional/"
  }
}